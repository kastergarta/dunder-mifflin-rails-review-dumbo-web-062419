<%= @employee.first_name %><br>
<%= @employee.last_name %><br>
<%= @employee.alias %><br>
<%= @employee.title %><br>
<%= @employee.office %><br>
<img src="<%= @employee.img_url %>"><br>

<% @dogs.select do |dog|%>
<% if dog.id == @employee.dog_id %>
